{
  "name": "webqabot",
  "scripts": {},
  "env": {
    "HUBOT_BUGZILLA_URL": {
      "required": true
    },
    "HUBOT_GITHUB_EVENT_NOTIFIER_ROOM": {
      "value": "#webqa-test"
    },
    "HUBOT_GITHUB_EVENT_NOTIFIER_TYPES": {
      "required": true
    },
    "HUBOT_IRC_NICK": {
      "value": "webqabot-review-app"
    },
    "HUBOT_IRC_ROOMS": {
      "value": "#webqa-test"
    },
    "HUBOT_IRC_SERVER": {
      "required": true
    },
    "HUBOT_LOG_LEVEL": {
      "value": "debug"
    },
    "HUBOT_WELCOME_MESSAGE": {
      "value": "Hey {nick}, welcome to this review app channel!"
    },
    "HUBOT_SEEN_TIMEAGO": {
      "value": "true"
    }    
  },
  "addons": [
    "heroku-redis",
    "papertrail"
  ]
}
